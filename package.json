{
  "name": "fogmachine",
  "version": "0.0.1",
  "description": "music streaming server",
  "main": "cli-boot-wrapper.js",
  "bin": {
    "fogmachine": "cli-boot-wrapper.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "node cli-boot-wrapper.js",
    "wizard": "node cli-boot-wrapper.js --wizard",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IrosTheBeggar/fogmachine-node"
  },
  "author": {
    "name": "Paul Sori",
    "email": "paul@fogmachine.io"
  },
  "homepage": "https://fogmachine.io/",
  "license": "GPL-3.0",
  "build": {
    "appId": "io.fogmachine.app",
    "productName": "Fog Machine",
    "electronVersion": "4.2.2",
    "asar": false,
    "files": [
      "**/*",
      "!dist/*",
      "!frp/*",
      "!.git/*",
      "!.vscode/*",
      "frp/readme.md"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "IrosTheBeggar",
        "repo": "fog"
      }
    ],
    "mac": {
      "files": [
        "frp/fogmachine-ddns-osx"
      ],
      "category": "public.app-category.Utilities",
      "binaries": [
        "frp/fogmachine-ddns-osx"
      ]
    },
    "win": {
      "files": [
        "frp/fogmachine-ddns-win.exe"
      ]
    },
    "linux": {
      "files": [
        "frp/fogmachine-ddns-linux"
      ]
    }
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "auto-launch": "^5.0.5",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "commander": "^3.0.1",
    "express": "^4.17.1",
    "make-dir": "^3.0.0",
    "serve-index": "^1.9.1",
    "winston": "^3.2.1"
  }
}
